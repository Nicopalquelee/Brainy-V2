{
  "name": "portal-academico-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nest start --watch",
    "build": "tsc -p tsconfig.build.json",
    "check:supabase": "ts-node -r tsconfig-paths/register scripts/check-supabase-admin.ts",
  "dev:register": "ts-node scripts/test-register.ts",
  "check:notes": "ts-node -r tsconfig-paths/register scripts/test-notes.ts",
    "dev:chat": "ts-node scripts/test-chat.ts",
  "fix:titles": "ts-node -r tsconfig-paths/register scripts/backfill-conversation-titles.ts",
    "test": "cross-env JEST_JUNIT_OUTPUT=test-results.xml jest --runInBand",
    "test:coverage": "cross-env JEST_JUNIT_OUTPUT=test-results.xml jest --runInBand --coverage --coverageReporters=lcov --coverageReporters=text-summary",
    "lint": "eslint src/**/*.ts test/**/*.ts --fix",
    "lint:report": "eslint src/**/*.ts test/**/*.ts --format json --output-file eslint-report.json",
    "format": "prettier --write '{src,test}/**/*.{ts,js,json,md}'"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^2.0.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@supabase/supabase-js": "^2.75.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^2.0.2",
    "node-fetch": "^2.6.7",
    "openai": "^5.23.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.19.127",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "cross-env": "^10.0.0",
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "jest-junit": "^16.0.0",
    "prettier": "^2.8.0",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.0"
  }
}
